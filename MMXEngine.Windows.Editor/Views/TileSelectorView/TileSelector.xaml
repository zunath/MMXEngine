<UserControl x:Class="MMXEngine.Windows.Editor.Views.TileSelectorView.TileSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MMXEngine.Windows.Editor.Views.TileSelectorView"
             xmlns:mvvm="http://prismlibrary.com/"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             mvvm:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Image
            Source="{Binding Texture}"
            Width="{Binding Texture.Width}"
            Height="{Binding Texture.Height}"
            Stretch="Fill"
            HorizontalAlignment="Left"
            VerticalAlignment="Top">
        </Image>
    </Grid>
</UserControl>
